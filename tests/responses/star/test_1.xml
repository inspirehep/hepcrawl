   <mets:dmdSec ID="ABES.STAR.THESE_62162.DESCRIPTION_BIBLIOGRAPHIQUE">
      <mets:mdWrap MDTYPE="OTHER" OTHERMDTYPE="tef_desc_these">
         <mets:xmlData>
            <tef:thesisRecord>
               <dc:title xml:lang="fr">Parallélisation de simulations interactives de champs ultrasonores pour le contrôle non destructif</dc:title>
               <dcterms:alternative xml:lang="en">Parallelization of ultrasonic field simulations for non destructive testing</dcterms:alternative>
               <dc:subject xml:lang="fr">Contrôle non destructif</dc:subject>
               <dc:subject xml:lang="fr">Programmation parallèle</dc:subject>
               <dc:subject xml:lang="fr">Simulation de champ ultrasonore</dc:subject>
               <dc:subject xml:lang="fr">Processeurs généralistes multicoeurs</dc:subject>
               <dc:subject xml:lang="fr">Processeurs graphiques</dc:subject>
               <dc:subject xml:lang="fr">GPGPU</dc:subject>
               <dc:subject xml:lang="fr">SIMD</dc:subject>
