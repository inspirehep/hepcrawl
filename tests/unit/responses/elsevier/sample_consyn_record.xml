	 			<ce:text>Heavy quarkonia</ce:text>
	 		</ce:keyword>
	 		<ce:keyword id="kw0020">
	 			<ce:text>Quark gluon plasma</ce:text>
	 		</ce:keyword>
	 		<ce:keyword id="kw0030">
	 			<ce:text>Mott effect</ce:text>
	 		</ce:keyword>
	 		<ce:keyword id="kw0040">
	 			<ce:text>X(3872)</ce:text>
	 		</ce:keyword>
